        <setColumnRemarks tableName="aigc_model" columnName="api_key" remarks="API密钥"/>
        <setColumnRemarks tableName="aigc_model" columnName="base_url" remarks="基础URL"/>
        <setColumnRemarks tableName="aigc_model" columnName="secret_key" remarks="密钥"/>
