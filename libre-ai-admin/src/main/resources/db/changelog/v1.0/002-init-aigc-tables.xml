            </column>
            <column name="gemini_location" type="VARCHAR(100)">
                <constraints nullable="true"/>
            </column>
            <column name="image_size" type="VARCHAR(50)">
                <constraints nullable="true"/>
            </column>
            <column name="image_quality" type="VARCHAR(50)">
                <constraints nullable="true"/>
            </column>
            <column name="image_style" type="VARCHAR(50)">
                <constraints nullable="true"/>
            </column>
            <column name="dimension" type="INT">
                <constraints nullable="true"/>
